#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/envinfo@7.8.1/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules:/mnt/d/myProgramming/computer-graphics/demos/node_modules:/mnt/d/myProgramming/computer-graphics/node_modules:/mnt/d/myProgramming/node_modules:/mnt/d/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules/envinfo/dist/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules/envinfo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/envinfo@7.8.1/node_modules/envinfo/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/envinfo@7.8.1/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules:/mnt/d/myProgramming/computer-graphics/demos/node_modules:/mnt/d/myProgramming/computer-graphics/node_modules:/mnt/d/myProgramming/node_modules:/mnt/d/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules/envinfo/dist/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules/envinfo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../envinfo/dist/cli.js" "$@"
fi
