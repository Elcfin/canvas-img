#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.71.0/node_modules/webpack-cli/bin/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.71.0/node_modules/webpack-cli/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.71.0/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules:/mnt/d/myProgramming/computer-graphics/demos/node_modules:/mnt/d/myProgramming/computer-graphics/node_modules:/mnt/d/myProgramming/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.71.0/node_modules/webpack-cli/bin/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.71.0/node_modules/webpack-cli/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack-cli@4.9.2_webpack@5.71.0/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules:/mnt/d/myProgramming/computer-graphics/demos/node_modules:/mnt/d/myProgramming/computer-graphics/node_modules:/mnt/d/myProgramming/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
