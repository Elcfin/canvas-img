#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/bin/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack@5.71.0/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules:/mnt/d/myProgramming/computer-graphics/demos/node_modules:/mnt/d/myProgramming/computer-graphics/node_modules:/mnt/d/myProgramming/node_modules:/mnt/d/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/terser-webpack-plugin@5.3.1_webpack@5.71.0/node_modules/terser-webpack-plugin/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/terser-webpack-plugin@5.3.1_webpack@5.71.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/bin/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack@5.71.0/node_modules/webpack/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/webpack@5.71.0/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules:/mnt/d/myProgramming/computer-graphics/demos/node_modules:/mnt/d/myProgramming/computer-graphics/node_modules:/mnt/d/myProgramming/node_modules:/mnt/d/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/terser-webpack-plugin@5.3.1_webpack@5.71.0/node_modules/terser-webpack-plugin/node_modules:/mnt/d/myProgramming/computer-graphics/demos/canvas-img/node_modules/.pnpm/terser-webpack-plugin@5.3.1_webpack@5.71.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.71.0/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.71.0/node_modules/webpack/bin/webpack.js" "$@"
fi
